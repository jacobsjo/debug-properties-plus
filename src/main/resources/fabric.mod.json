{
  "schemaVersion": 1,
  "id": "debug-properties-plus",
  "version": "${version}",
  "name": "Debug Properties Plus",
  "description": "A convenient UI and command to modify debug properties at runtime.",
  "authors": [
    {
      "name": "jacobsjo",
      "contact": {
        "homepage": "https://jacobsjo.eu"
      }
    }
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/debug-properties-plus",
    "issues": "https://github.com/jacobsjo/debug-properties-plus/issues",
    "sources": "https://github.com/jacobsjo/debug-properties-plus"
  },
  "license": "MIT",
  "icon": "assets/debug-properties-plus/logo.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "eu.jacobsjo.debugPropertiesPlus.client.DebugPropertiesPlusClient"
    ],
    "main": [
      "eu.jacobsjo.debugPropertiesPlus.DebugPropertiesPlus"
    ]
  },
  "mixins": [
    "debug-properties-plus.mixins.json",
    {
      "config": "debug-properties-plus.client.mixins.json",
      "environment": "client"
    }
  ],
  "accessWidener": "debug-properties-plus.classtweaker",
  "depends": {
    "fabricloader": ">=0.18.3",
    "fabric-api": ">=0.140.3",
    "minecraft": "~26.1-"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.kofi": "https://ko-fi.com/jacobsjo",
        "modmenu.github_sponsors": "https://github.com/sponsors/jacobsjo"
      }
    }
  }
